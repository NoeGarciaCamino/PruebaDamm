<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="DAMM" parallel="tests" thread-count="2">

    <listeners>
        <listener class-name="com.aventstack.extentreports.testng.listener.ExtentITestListenerClassAdapter"/>
    </listeners>
    <test name="Google_Pixel_3a_POC03">
<!--        <parameter name="udid" value="Google_Pixel_3a_POC03"/>-->
        <parameter name="udid" value="Google_Pixel_3a_real"/>
        <parameter name="port" value="443"/>
        <parameter name="platformName" value="android"/>
        <classes>
            <class name="com.everis.ct.mobile.runner.RunnerGooglePixel"/>
        </classes>
    </test>
    <test name="iPhone 12 mini">
           <parameter name="udid" value="iPhone_12_mini_14_POC12"/>
        <parameter name="udid" value="iPhone_12_mini_14_real"/>
        <parameter name="port" value="443"/>
        <parameter name="platformName" value="ios"/>
        <classes>
            <class name="com.everis.ct.mobile.runner.RunneriPhone12mini"/>
        </classes>
    </test>
</suite>